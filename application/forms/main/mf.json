{
    "name": "main_form",
    "className": "TForm",
    "caption": "Delphi.js Boilerplate - Main Form. It is sizeable and draggable",
    "top": 50,
    "right": 50,
    "bottom": 50,
    "left": 50,
    "minWidth": 700,
    "minHeight": 600,
    "sizeable": true,
    "visible": true,
    "scripts": [
        "application/forms/main/mf_logics.js"
    ],
    "TComponents": {
        "myToolBar": {
            "className": "TToolBar",
            "top": 0,
            "height": 30,
            "TComponents": {
                "addRecordToolButton": {
                    "className": "TToolButton",
                    "iconBase64": {
                        "type": "png",
                        "data": ""
                    },
                    "toolTip": "Добавить новую запись",
                    "height": 24,
                    "width": 24
                },
                "editRecordToolButton": {
                    "className": "TToolButton",
                    "icon": "application/resources/images/icons/record_update.png",
                    "enabled": false,
                    "height": 24,
                    "width": 24
                },
                "deleteRecordToolButton": {
                    "className": "TToolButton",
                    "icon": "application/resources/images/icons/record_delete.png",
                    "enabled": false,
                    "height": 24,
                    "width": 24
                },
                "myToolBarSeparator1": {
                    "className": "TToolBarSeparator",
                    "height": 24,
                    "width": 0
                },
                "getUSDRateButton": {
                    "className": "TToolButton",
                    "toolTip": "Получить курс доллара к рублю",
                    "iconBase64": {
                        "type": "png",
                        "data": ""
                    },
                    "height": 24,
                    "width": 24
                },
                "getEuroDRateButton": {
                    "className": "TToolButton",
                    "toolTip": "Получить курс евро к рублю",
                    "iconBase64": {
                        "type": "png",
                        "data": ""
                    },
                    "height": 24,
                    "width": 24
                }
            }
        },
        "firstPanel": {
            "className": "TPanel",
            "caption": "",
            "top": 32,
            "right": 0,
            "height": 160,
            "left": 0,
            "border": false,
            "depressed": false,
            "TComponents": {
                "aboutButton": {
                    "className": "TBitButton",
                    "iconBase64": {
                        "type": "png",
                        "data": ""
                    },
                    "enabled": true,
                    "caption": "Non-modal",
                    "height": 26,
                    "top": 40,
                    "width": 120,
                    "right": 4,
                    "textAlign": "center"
                },
                "aboutButtonModal": {
                    "className": "TBitButton",
                    "icon": "application/resources/images/icons/question.png",
                    "enabled": false,
                    "caption": "Show modal",
                    "height": 26,
                    "top": 70,
                    "width": 120,
                    "right": 4,
                    "textAlign": "right"
                },
                "concatenateButton": {
                    "className": "TButton",
                    "caption": "Concatenate",
                    "toolTip": "Press me to concatenate two strings",
                    "height": 26,
                    "top": 10,
                    "width": 120,
                    "right": 4,
                    "textAlign": "left"
                },
                "editStringOne": {
                    "className": "TEdit",
                    "right": 130,
                    "height": 26,
                    "top": 10,
                    "left": 108,
                    "fontStyle": "italic",
                    "placeholder": "TEdit with italic font"
                },
                "editStringTwo": {
                    "className": "TEdit",
                    "right": 130,
                    "height": 26,
                    "top": 40,
                    "left": 108,
                    "placeholder": "TEdit with regular font"
                },
                "comboBoxOne": {
                    "className": "TComboBox",
                    "right": 130,
                    "height": 26,
                    "top": 70,
                    "left": 108,
                    "items": [
                        [
                            "0",
                            "This is default (or zero) element"
                        ],
                        [
                            "1",
                            "This is first element"
                        ],
                        [
                            "2",
                            "Second element"
                        ],
                        [
                            "3",
                            "Third of us"
                        ],
                        [
                            "4",
                            "Magnificent Four"
                        ],
                        [
                            "5",
                            "5-th element"
                        ]
                    ],
                    "initialValue": "3"
                },
                "labelStringOne": {
                    "className": "TStaticText",
                    "caption": "First string",
                    "width": 100,
                    "height": 26,
                    "top": 10,
                    "left": 4,
                    "textAlign": "right"
                },
                "labelStringTwo": {
                    "className": "TStaticText",
                    "caption": "Second string",
                    "width": 100,
                    "height": 26,
                    "top": 40,
                    "left": 4,
                    "textAlign": "center"
                },
                "labelStringThree": {
                    "className": "TStaticText",
                    "caption": "Combo caption",
                    "width": 100,
                    "height": 26,
                    "top": 70,
                    "left": 4,
                    "textAlign": "left"
                },
                "labelResult": {
                    "className": "TStaticText",
                    "caption": "Multiline colored TStatic text. Try to fill First string and Second string, then press Concatenate button for result.",
                    "top": 102,
                    "left": 2,
                    "right": 2,
                    "height": 48,
                    "textAlign": "center",
                    "color": "maroon",
                    "multiLine": true
                }
            }
        },
        "secondPanel": {
            "className": "TPanel",
            "caption": "Panel with caption as a container for TRadioButton, TCheckBox and TMemo",
            "captionColor": "navy",
            "top": 204,
            "right": 1,
            "bottom": 1,
            "left": 1,
            "TComponents": {
                "firstCheckBox": {
                    "className": "TCheckBox",
                    "caption": "First CheckBox",
                    "height": 24,
                    "width": 120,
                    "top": 10,
                    "left": 8
                },
                "secondCheckBox": {
                    "className": "TCheckBox",
                    "caption": "Second CheckBox",
                    "enabled": false,
                    "height": 24,
                    "width": 140,
                    "top": 10,
                    "left": 140
                },
                "firstRadioButton": {
                    "className": "TRadioButton",
                    "caption": "Do it once",
                    "height": 24,
                    "width": 100,
                    "top": 40,
                    "left": 8
                },
                "secondRadioButton": {
                    "className": "TRadioButton",
                    "caption": "Do it twice",
                    "height": 24,
                    "width": 100,
                    "top": 40,
                    "left": 108
                },
                "thirdRadioButton": {
                    "className": "TRadioButton",
                    "caption": "Do it three",
                    "height": 24,
                    "width": 100,
                    "top": 40,
                    "left": 208
                },
                "fourthRadioButton": {
                    "className": "TRadioButton",
                    "caption": "Do it four",
                    "height": 24,
                    "width": 100,
                    "top": 40,
                    "left": 308
                },
                "myMemo": {
                    "className": "TMemo",
                    "placeholder": "Just write on something ...",
                    "top": 70,
                    "left": 0,
                    "right": 0,
                    "bottom": 0,
                    "color": "maroon"
                }
            }
        }
